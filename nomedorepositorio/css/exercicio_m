#include<stdio.h>
void main()
{
    char SEXO;
    char NOME[50];
    printf("Digite seu sexo: ");
    scanf(" %c", &SEXO);
    printf("Digite seu nome(Se composto, usar ''_'' como espaco): ");
    scanf("%s", NOME);
    switch(SEXO)
    {
        case 'M':
        printf("Ilmo. Sr.%s", NOME);break;
        case 'F':
        printf("Ilma. Sra.%s", NOME);break;
        default:printf("Sexo informado invalido");
    }
}
