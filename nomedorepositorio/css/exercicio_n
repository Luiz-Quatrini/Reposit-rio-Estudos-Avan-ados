#include<stdio.h>
#include<math.h>
int main()
{
    int A, B, C, D;
    printf("Digite o valor de A: \n");
    scanf("%d", &A);
    printf("Digite o valor de B: \n");
    scanf("%d", &B);
    printf("Digite o valor de C: \n");
    scanf("%d", &C);
    D=A+B+C;
    if(D>=100)
    {
        printf("O valor da soma das 3 variaveis eh: %d", D);
    }
    else
    {
        printf("Erro.");
    }
}
