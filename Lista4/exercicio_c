#include <stdio.h>
int main()
{
    int i;
    int vetorA[20]; int vetorB[20]; int vetorC[20];
    for(i=0;i<20;i++)
    {
        vetorA[i]=i;
        vetorB[i]=i;
    }
    for(i=0;i<20;i++)
    {
        vetorC[i]= vetorA[i]-vetorB[i];
    }
    for(i=0;i<20;i++)
    {
        printf("%d\n", vetorC[i]);
    }
}
