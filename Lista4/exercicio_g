#include <stdio.h>
int main()
{
   int vetorA[20], vetorB[20];
   int i;
   for(i = 0; i < 20; i++)
   {
       vetorA[i]=i;
   }
   for(i = 0; i < 20; i++)
   {
       vetorB[i] = vetorA[19 - i];
   }
   for(i = 0; i < 20; i++)
   {
       printf("vetorA[%d] = %d vetorB[%d] = %d\n", i, vetorA[i], i, vetorB[i]);
   }
}
